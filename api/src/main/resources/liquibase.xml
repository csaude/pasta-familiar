			</column>

			<column name="date_created" type="datetime">
				<constraints nullable="false" />
			</column>

			<column name="retired" type="int(11)">
				<constraints nullable="false" />
			</column>

			<column name="retired_by" type="int(11)">
				<constraints nullable="true" />
			</column>

			<column name="date_retired" type="datetime">
				<constraints nullable="true" />
